{{>header}}

<nav class="navbar bg-secondary" id="bglog">
  <a class="navbar-brand"> PS5 </a>
  <div class="container-fluid"></div>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav">
        <li class="nav-item">
          <a class="nav-link active" aria-current="page" href="#">Home</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Features</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="#">Pricing</a>
        </li>
        <li class="nav-item">
          <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
        </li>
      </ul>
    </div>
  </div>
  <form action="/logout">
    <button id="logout" class="btn  my-2 my-sm-0" type="submit" onclick="doIt()">Logout</button>
  </form>
</nav>
<div id="backgroundclr">
  <div class="container-fluid">
    <div class="row">
      {{#each items}}
      <div class="card" style="width: 22rem; margin:13px; background-color: azure">
        <img class="card-img-top" src="{{image}}" alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title">{{cardtitle}}</h5>
          <p class="card-text">{{discription}}</p>
          <a href="#" class="btn btn-primary" style="">{{button}}</a>
        </div>
      </div>
      {{/each}}
    </div>
  </div>
</div>
{{>footer}}
<script>
  function doIt(){

  Swal.fire({
    position: 'top-end',
    icon: 'error',
    title: 'Logged out!',
    showConfirmButton: false,
  })


  }
   
</script>